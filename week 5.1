def remove_dirty_chars(s1, s2):
    count = [0] * 256
    for char in s2:
        count[ord(char)] += 1
    ip_idx, res = 0, ""

    while ip_idx < len(s1):
        temp = s1[ip_idx]
        if count[ord(temp)] == 0:
            res += temp
        ip_idx += 1

    return res


sample_input1 = input()
sample_input2 = input()
result = remove_dirty_chars(sample_input1, sample_input2)
print(result) 
